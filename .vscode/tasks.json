{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "cleanapi-cyphertest",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/src/CypherTest/CypherTest.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "cleanapi-Default",
            "command": "dotnet",
            "type": "process",
            "args": [
                "clean",
                "${workspaceFolder}/src/BackEnd.Api/BackEnd.Api.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "buildapi-cyphertest",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/CypherTest/CypherTest.csproj"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn":["cleanapi-cyphertest"]
        },

        {
            "label": "buildapi-BackEnd",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/BackEnd.Api/BackEnd.Api.csproj"
            ],
            "problemMatcher": "$msCompile",
            "dependsOn":["cleanapi-BackEnd"]
        },
        
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/test/unit/Cypher.UnitTest/Cypher.UnitTest.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/test/unit/Cypher.UnitTest/Cypher.UnitTest.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "test",
            "command": "dotnet",
            "type": "process",
            "args": [
                "test",
                "/p:CollectCoverage=true",
                "/p:CoverletOutputFormat=lcov",
                "/p:CoverletOutput=../../../coverage/lcov.info"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}